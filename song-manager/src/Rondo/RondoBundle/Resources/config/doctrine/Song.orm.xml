<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Rondo\RondoBundle\Entity\Song" table="songs">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="false"/>
    <field name="text" type="text" column="text" nullable="true"/>
    <field name="image" type="blob" column="image" nullable="true"/>
    <field name="pagerondored" type="integer" column="pageRondoRed" nullable="true"/>
    <field name="pagerondoblue" type="integer" column="pageRondoBlue" nullable="true"/>
    <field name="pagerondogreen" type="integer" column="pageRondoGreen" nullable="true"/>
    <field name="islicensefree" type="boolean" column="isLicenseFree" nullable="false"/>
    <field name="copyrightinfo" type="text" column="copyrightInfo" nullable="true"/>
    <field name="status" type="integer" column="status" nullable="false"/>
  </entity>
</doctrine-mapping>
