<!DOCTYPE html>
<html ng-app="RondoApp">
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link href="{{ asset('bundles/rondo/css/lib/bootstrap.3.3.4.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/rondo/css/style.css') }}" rel="stylesheet">
    {% endblock %}
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}
    <script src="{{ asset('bundles/rondo/js/lib/angular.1.3.14.min.js') }}"></script>
    <script src="{{ asset('bundles/rondo/js/lib/ui-bootstrap.0.12.1.min.js') }}"></script>
    <script src="{{ asset('bundles/rondo/js/lib/angular-route.1.3.14.min.js') }}"></script>
    <script src="{{ asset('bundles/rondo/js/lib/angular-file-upload.1.1.5.min.js') }}"></script>
    <script src="{{ asset('bundles/rondo/js/app/app.js') }}"></script>
    <script src="{{ asset('bundles/rondo/js/app/controller/SongListController.js') }}"></script>
    <script src="{{ asset('bundles/rondo/js/app/controller/SongDetailController.js') }}"></script>
{% endblock %}
</body>
</html>
